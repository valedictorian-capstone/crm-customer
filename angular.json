{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "crm-customer": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss",
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "allowedCommonJsDependencies": [
                            "lodash",
                            "xlsx",
                            "@ant-design/colors",
                            "chart.js",
                            "echarts",
                            "@firebase/auth",
                            "@firebase/util",
                            "@firebase/component",
                            "firebase/app",
                            "@firebase/app",
                            "@firebase/database",
                            "firebase",
                            "hammerjs"
                        ],
                        "outputPath": "dist/crm-customer",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/firebase-messaging-sw.js",
                            {
                                "glob": "**/*",
                                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                                "output": "/assets/"
                            },
                            "src/manifest.webmanifest",
                            "src/web.config"
                        ],
                        "styles": [
                            "node_modules/angular-calendar/css/angular-calendar.css",
                            "src/styles.scss",
                            "src/assets/styles/ant.theme.less",
                            "src/assets/styles/bootstrap.theme.scss",
                            "src/assets/styles/fontawesome.theme.scss",
                            "src/assets/styles/nebular.theme.scss",
                            "src/assets/styles/prime.theme.scss"
                        ],
                        "scripts": [
                            "node_modules/quill/dist/quill.js",
                            "src/assets/latest.sdk.bundle.min.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "10mb",
                                    "maximumError": "10mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "20kb",
                                    "maximumError": "20kb"
                                }
                            ],
                            "serviceWorker": true,
                            "ngswConfigPath": "ngsw-config.json"
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "crm-customer:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "crm-customer:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "crm-customer:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/manifest.webmanifest"
                        ],
                        "styles": [
                            "node_modules/angular-calendar/css/angular-calendar.css",
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.app.json",
                            "tsconfig.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "crm-customer:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "crm-customer:serve:production"
                        }
                    }
                },
                "deploy": {
                    "builder": "@angular/fire:deploy",
                    "options": {}
                }
            }
        }
    },
    "defaultProject": "crm-customer",
    "cli": {
        "analytics": "93641f1d-5d07-4ab9-9978-8bd50b4e004a",
        "defaultCollection": "@ngrx/schematics"
    }
}